<table class="table table-bordered ">
<col width="3%">
<col width="15%">
<col width="12%">
<col width="20%">
<col width="12%">
<col width="20%">
<col width="8%">
<col width="10%">
<thead bgcolor='#026c48'>
  <td align="center"><font color="#fff">STT</font></td>
  <td align="center"><font color="#fff">Nhân viên</font></td>
  <td align="center"><font color="#fff">CheckIn</font></td>
  <td align="center"><font color="#fff">Địa điểm CheckIn</font></td>
  <td align="center"><font color="#fff">CheckOut</font></td>
  <td align="center"><font color="#fff">Địa điểm CheckOut</font></td>
  <td align="center"><font color="#fff">Trạng thái</font></td>
  <td align="center"><font color="#fff">Pin(%)</font></td>
</thead>
<tbody>
  <% users.each_with_index do |user, index| %>
    <tr>
    <td><%= (index + 1).to_s %></td>
    <td><%= link_to user[:name], user_path(user[:uid]) %></td>
    <td><%= "#{user[:ci_date]} #{user[:ci_time]}" %></td>
    <td><%= user[:ci_address] %></td>
    <td><%= "#{user[:co_date]} #{user[:co_time]}" %></td>
    <td><%= user[:co_address] %></td>
    <td><%= user[:online_status] == false ? 'offline' : 'online' %></td>
    <td><%= user[:percentage] %></td>
    </tr>
  <% end %>
</tbody>
</table>
<%= paginate users, remote: true, theme: 'twitter-bootstrap-3' %>

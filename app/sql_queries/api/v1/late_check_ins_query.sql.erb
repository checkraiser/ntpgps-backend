select groups.name group_name,users.name,s.d1,s.d2,s.d3,s.d4,s.d5,s.d6,s.d7,s.d8,s.d9,s.d10,s.d11,s.d12,s.d13,s.d14,s.d15,s.d16,s.d17,s.d18,s.d19,s.d20,s.d21,s.d22,s.d23,s.d24,s.d25,s.d26,s.d27,s.d28,s.d29,s.d30,s.d31
from (
select * 
from crosstab(

 'select user_id,check_in_month,check_in_date,late from check_in_late_views Where check_in_month= <%= @month %> Order by 1',
 'select d from generate_series(1,31) d'
) as (
  user_id int,
  check_in_month int,
  "d1" float,
  "d2" float,
  "d3" float,
  "d4" float,
  "d5" float,
  "d6" float,
  "d7" float,
  "d8" float,
  "d9" float,
  "d10" float,
  "d11" float,
  "d12" float,
  "d13" float,
  "d14" float,
  "d15" float,
  "d16" float,
  "d17" float,
  "d18" float,
  "d19" float,
  "d20" float,
  "d21" float,
  "d22" float,
  "d23" float,
  "d24" float,
  "d25" float,
  "d26" float,
  "d27" float,
  "d28" float,
  "d29" float,
  "d30" float,
  "d31" float  
)
) s
inner join users on s.user_id=users.id
inner join user_groups on s.user_id=user_groups.user_id
inner join groups on user_groups.group_id=groups.id
Order by groups.name,users.name 
